___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require("logToConsole");
const queryPermission = require("queryPermission");
const injectScript = require("injectScript");
const createQueue = require("createQueue");
const getUrl = require("getUrl");
const makeTableMap = require("makeTableMap");

const apiAllowed = queryPermission("access_globals", "readwrite", "_gsc");
const _gsc = apiAllowed ? createQueue("_gsc") : function () {};

if (!data.siteId) {
    onFailure("site id is empty");
} else {
    if (apiAllowed) {
        if (data.followHistoryChange && queryPermission("get_url")) {
            _gsc(["trackPage", getUrl()]);
        }
        if (data.trackGTM) {
            _gsc(["settings", {"trackGTM": true}]);
        }
        if (data.params) {
            _gsc(["params", makeTableMap(data.params, "name", "value")]);
        }
    }

    const scriptUrl = "https://l.getsitecontrol.com/" + data.siteId + ".js";
    if (queryPermission("inject_script", scriptUrl)) {
        injectScript(scriptUrl, onSuccess, onFailure, scriptUrl);
    } else {
        onFailure("injectScript permission denied");
    }
}

function onFailure(msg) {
    if (msg) { log(msg); }
    data.gtmOnFailure();
}

function onSuccess() {
    data.gtmOnSuccess();
}


